<% if(theme.site_verification.google) { %>
    <!-- Site Verification -->
    <% for (serve in theme.site_verification.google) { %>
        <meta name="google-site-verification" content="<%= theme.site_verification.google[serve] %>" />
    <% } %>
<% } %>
<% if(theme.site_verification.baidu) { %>
    <!-- Site Verification -->
    <% for (serve in theme.site_verification.baidu) { %>
        <meta name="baidu-site-verification" content="<%= theme.site_verification.baidu[serve] %>" />
    <% } %>
<% } %>